<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.e2.SignInController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="133.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
      <Text fill="#f8f5f5" layoutX="202.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tickets App">
         <font>
            <Font size="38.0" />
         </font>
      </Text>
      <TextField fx:id="txtUserName" layoutX="225.0" layoutY="164.0" promptText="user name" />
      <TextField fx:id="txtUserPassword" layoutX="226.0" layoutY="206.0" promptText="password" />
      <Button fx:id="bthSignIn" layoutX="225.0" layoutY="240.0" mnemonicParsing="false" onAction="#onSignIn" prefHeight="25.0" prefWidth="71.0" text="sign in" />
      <Button fx:id="bthlogin" layoutX="303.0" layoutY="240.0" mnemonicParsing="false" onAction="#onLoginClicked" prefHeight="25.0" prefWidth="71.0" text="login" />
   </children>
</AnchorPane>
